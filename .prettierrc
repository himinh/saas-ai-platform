{
	"printWidth": 80,
	"endOfLine": "auto",
	"tabWidth": 2,
	"quoteProps": "consistent",
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"requirePragma": false,
	"proseWrap": "always",
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
